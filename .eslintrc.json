{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 13,
        "parser": "babel-eslint",
        "sourceType": "module"
    },
    "rules": {
        "linebreak-style": "off",
        "operator-linebreak": "off",
        "no-param-reassign": "off",
        "import/prefer-default-export": "off",
        "implicit-arrow-linebreak": "off",
        "object-curly-newline": "off"
    }
}